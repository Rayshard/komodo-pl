(program MyProgram
    (entry MyModule Main)
    (module MyModule
        (function Main (params) (returns)
            (Call MyModule TestArgs 123 456 true)

            (Exit 0)
        )
        (function TestArgs (params I64 (I64 myArgI64) (Bool myArgBool)) (returns)
            (Assert (arg 0) 123)
            (Assert (arg myArgI64) 456)
            (Assert (arg myArgBool) true)

            (Return)
        )
    )
)