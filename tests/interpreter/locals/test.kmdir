(program MyProgram
    (entry MyModule Main)
    (module MyModule
        (function Main (params) (returns)
            (locals
                (I64 localI64)
            )

            (GetLocal localI64)
            (PushConstant 0)
            (Assert EQ)

            (PushConstant 1)
            (SetLocal localI64)

            (GetLocal localI64)
            (PushConstant 1)
            (Assert EQ)

            (Exit 0)
        )
    )
)