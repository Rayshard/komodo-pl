(program MyProgram
    (entry MyModule Main)
    (module MyModule
        (global I64 myGlobal)

        (function Main (params) (returns)
            (Assert (global MyModule myGlobal) 0)

            (PushConstant 1)
            (SetGlobal MyModule myGlobal)
            (Assert (global MyModule myGlobal) 1)

            (Exit 0)
        )
    )
)