(program MyProgram
    (entry MyModule Main)
    (module MyModule
        (globals
            (I64 myGlobal)
        )

        (function Main
            (Assert (global MyModule myGlobal) 0)

            (Store 1 (global MyModule myGlobal))
            (Assert (global MyModule myGlobal) 1)

            (Exit 0)
        )
    )
)