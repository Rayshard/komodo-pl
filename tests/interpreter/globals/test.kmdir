(program MyProgram
    (entry MyModule Main)
    (module MyModule
        (globals
            (I64 myDefaultGlobal)
            (Bool mySetGlobal true)
        )

        (function Main
            (Dump (global MyModule myDefaultGlobal))
            (Dump (global MyModule mySetGlobal))

            (Store 1 (global MyModule myDefaultGlobal))
            (Dump (global MyModule myDefaultGlobal))

            (Exit 0)
        )
    )
)