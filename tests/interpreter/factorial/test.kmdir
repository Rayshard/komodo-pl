(program MyProgram
    (entry MyModule Main)
    (module MyModule
        (function Main
            (args)
            (locals)
            (ret Unit)
            (basicBlock __entry__
                (PushI64 5)
                (Call MyModule Factorial)
                (Print)
                (PushI64 0)
                (Syscall Exit)
            )
        )
        (function Factorial
            (args I64)
            (locals)
            (ret I64)
            (basicBlock __entry__
                (LoadArg 0)
                (PushI64 0)
                (Eq)
                (JNZ retOneOnZero)
                (LoadArg 0)
                (LoadArg 0)
                (Dec)
                (Call MyModule Factorial)
                (Mul)
                (Return)
            )
            (basicBlock retOneOnZero
                (PushI64 1)
                (Return)
            )
        )
    )
)