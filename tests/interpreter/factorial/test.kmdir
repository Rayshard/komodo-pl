            (args)
            (locals I64)
            (returns)
            (basicBlock __entry__
                (Call MyModule Factorial 20 ~ $local0)
                (Print I64 $local0)
                
                (Load 0)
                (Syscall Exit)
            )
        )
        (function Factorial
            (args I64)
            (locals Bool I64 I64 I64)
