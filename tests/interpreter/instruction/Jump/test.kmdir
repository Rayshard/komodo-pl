(program MyProgram
    (entry MyModule Main)
    (module MyModule
        (function Main (params) (returns)
            (Jump badExit false)
            (Jump goodExit true)

            (Exit 2)

          (label badExit)
            (Exit 1)

          (label goodExit)
            (Jump exit)
            (Exit 3)
            
          (label exit)
            (Exit 0)
        )
    )
)