(program MyProgram
    (entry MyModule Main)
    (module MyModule
        (function Main
            (args)
            (locals)
            (returns)
            (basicBlock __entry__
                (Push Bool false)
                (CJump badExit)
                (Push I64 123)
                (Print)

                (Push Bool true)
                (CJump goodExit)

                (Push I64 2)
                (Syscall Exit)
            )
            (basicBlock badExit
                (Push I64 1)
                (Syscall Exit)
            )
            (basicBlock goodExit
                (Push I64 456)
                (Print)

                (Push I64 0)
                (Syscall Exit)
            )
        )
    )
)