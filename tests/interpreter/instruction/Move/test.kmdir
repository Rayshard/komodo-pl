(program MyProgram
    (entry MyModule Main)
    (module MyModule
        (function Main (params) (returns)
            (locals)
            
            (Call MyModule TestMoveLocal)

            (Move 15 $stack)
            (Assert $stack 15)

            (Exit 0)
        )
        (function TestMoveLocal (params) (returns)
            (locals I64)

            (Move 55 (local 0))
            (Assert (local 0) 55)
            (Return)
        )
    )
)