(program MyProgram
    (entry MyModule Main)
    (module MyModule
        (function Main (params) (returns)
            (Call MyModule TestNoArgument)
            (Call MyModule TestSingleArgument 1)
            (Call MyModule TestMultipleArguments 2 3 4)

            (Call (func MyModule TestNoArgument))
            (Call (func MyModule TestSingleArgument) 1)
            (Call (func MyModule TestMultipleArguments) 2 3 4)

            (Exit 0)
        )
        (function TestNoArgument (params) (returns)
            (Dump 0)
            (Return)
        )
        (function TestSingleArgument (params I64) (returns)
            (Dump (arg 0))
            (Return)
        )
        (function TestMultipleArguments (params I64 I64 I64) (returns)
            (Dump (arg 0))
            (Dump (arg 1))
            (Dump (arg 2))
            (Return)
        )
    )
)