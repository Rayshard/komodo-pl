(program MyProgram
    (entry MyModule Main)
    (module MyModule
        (function Main
            (Call MyModule TestNoArgument)
            (Call MyModule TestSingleArgument 2)
            (Call MyModule TestMultipleArguments 3 4 5)

            (Exit 0)
        )
        (function TestNoArgument
            (Dump true)
            (Return)
        )
        (function TestSingleArgument (params I64)
            (Load (arg 0))
            (Assert $stack 2)
            (Return)
        )
        (function TestMultipleArguments (params I64 I64 I64)
            (Load (arg 0))
            (Assert $stack 3)

            (Load (arg 1))
            (Assert $stack 4)

            (Load (arg 2))
            (Assert $stack 5)

            (Return)
        )
    )
)