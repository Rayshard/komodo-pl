(program MyProgram
    (entry MyModule Main)
    (module MyModule
        (function Main
            (args)
            (locals)
            (returns)
            (basicBlock __entry__
                (Call MyModule TestNoArgument)

                (Push I64 2)
                (Call MyModule TestSingleArgument)

                (Push I64 5)
                (Push I64 4)
                (Push I64 3)
                (Call MyModule TestMultipleArguments)

                (Push I64 0)
                (Syscall Exit)
            )
        )
        (function TestNoArgument
            (args)
            (locals)
            (returns)
            (basicBlock __entry__
                (Push Bool True)
                (Print)
                (Return)
            )
        )
        (function TestSingleArgument
            (args I64)
            (locals)
            (returns)
            (basicBlock __entry__
                (LoadArg 0)
                (Assert I64 2)
                (Return)
            )
        )
        (function TestMultipleArguments
            (args I64 I64 I64)
            (locals)
            (returns)
            (basicBlock __entry__
                (LoadArg 0)
                (Assert I64 3)

                (LoadArg 1)
                (Assert I64 4)

                (LoadArg 2)
                (Assert I64 5)

                (Return)
            )
        )
    )
)