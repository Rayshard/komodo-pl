(program MyProgram
    (entry MyModule Main)
    (module MyModule
        (function Main
            (Store 123 $stack)
            (Call MyModule TestNoReturn)
            (Assert $stack 123)

            (Store 123 $stack)
            (Call MyModule TestSingleReturn)
            (Assert $stack 789)
            (Assert $stack 123)

            (Store 123 $stack)
            (Call MyModule TestMultipleReturn)
            (Assert $stack 10)
            (Assert $stack 11)
            (Assert $stack 12)
            (Assert $stack 123)

            (Exit 0)
        )
        (function TestNoReturn
            (Return)
        )
        (function TestSingleReturn (returns I64)
            (Return 789)
        )
        (function TestMultipleReturn (returns I64 I64 I64)
            (Return 10 11 12)
        )
    )
)