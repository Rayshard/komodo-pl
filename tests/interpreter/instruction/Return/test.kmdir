(program MyProgram
    (entry MyModule Main)
    (module MyModule
        (function Main (params) (returns)
            (Move 123 $stack)
            (Call MyModule TestNoReturn)
            (Assert (pop I64) 123)

            (Move 123 $stack)
            (Call MyModule TestSingleReturn)
            (Assert (pop I64) 789)
            (Assert (pop I64) 123)

            (Move 123 $stack)
            (Call MyModule TestMultipleReturn)
            (Assert (pop I64) 10)
            (Assert (pop I64) 11)
            (Assert (pop I64) 12)
            (Assert (pop I64) 123)

            (Exit 0)
        )
        (function TestNoReturn (params) (returns)
            (Return)
        )
        (function TestSingleReturn (params) (returns I64)
            (Return 789)
        )
        (function TestMultipleReturn (params) (returns I64 I64 I64)
            (Return 10 11 12)
        )
    )
)