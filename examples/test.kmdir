(program MyProgram
    (entry MyModule Main)
    (module System
        (function GetTime (params) (returns UI64)
            (Syscall GetTime)
            (Return $stack)
        )
    )

    (module MyModule
        (function Main (params) (returns)
            (Call System GetTime ~ $stack)
            (Dump $stack)
            (Exit 0)
        )
    )
)